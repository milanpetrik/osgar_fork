{
  "version": 2,
  "robot": {
    "modules": {
      "gps": {
          "driver": "tcp",
          "in": ["raw"],
          "out": ["raw"],
          "init": {
              "host": "192.168.2.10",
              "port": 10001,
              "bufsize": 2000
          }
      },
      "timer": {
          "driver": "timer",
          "in": [],
          "out": ["tick"],
          "init": {"sleep": 1.0}
      }
    },
    "links": [
      ["timer.tick", "gps.raw"]
      ]
  }
}

