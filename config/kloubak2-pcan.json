{
  "version": 2,
  "robot": {
    "modules": {
      "app": {
          "driver": "application",
          "in": ["encoders", "emergency_stop", "pose2d", "buttons"],
          "out": ["desired_speed"],
          "init": {
            "max_speed": 0.5
          }
      },
      "kloubak": {
          "driver": "kloubak",
          "in": ["can", "desired_speed"],
          "out": ["can", "encoders", "emergency_stop", "pose2d"],
          "init": {
            "drive_mode": "ALL",
            "wheel_distance": 0.496,
            "center_axle_distance": 0.348,
            "ad_center": 531,
            "ad_calibration_deg": 45,
            "ad_range": -178,
            "max_joint_angle_deg": 68
          }
      },
      "can": {
          "driver": "pcan",
          "in": ["can"],
          "out": ["can"],
          "init": {
            "can_filters": [{"can_id": 0, "can_mask": 0, "extended": false}]
          }
      }
    },
    "links": [["kloubak.can", "can.can"],
              ["can.can", "kloubak.can"],
              ["app.desired_speed", "kloubak.desired_speed"],
              ["kloubak.pose2d", "app.pose2d"],
              ["kloubak.emergency_stop", "app.emergency_stop"]
    ]
  }
}